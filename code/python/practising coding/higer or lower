import random
print("Hello! welcome to the guessing game. please guess between a number between 1 and 100")
print("what is your guess?:")
guess =int(input("what is your guess?:"))
correct_number =random.randint(1,100)
guess_count = 1
while guess !=correct_number:
    guess_count += 1
    if guess < correct_number:
        guess = int(input("wrong. You need to guess higher. What is your guess?:"))
    else:
        guess = int(input("worng! you need to guess a lower number"))
